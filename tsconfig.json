{
  "exclude": [
    "dist",
    "node_modules"
  ],
  "compilerOptions": {
    "target": "ESNEXT",
    "module": "commonjs",
    "allowJs": true,
    "outDir": "./dist",
    "strict": false,
    "noImplicitThis": false,
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}